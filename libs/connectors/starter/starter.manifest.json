{
    "id": "enter_unique_id_here",
    "version": "1.0.1",
    "author": "IBM",
    "name": "Starter Connector",
    "description": "A skeleton starter module for building new connectors.",
    "moduleFilePath": "/dist/libs/connectors/starter/src/starter-connector.module.js",
    "settings": [
        {
            "id": "i2.config",
            "value": "./libs/connectors/starter/static/config.json",
            "type": "FILE"
        },
        {
            "id": "i2.schemas",
            "value": "./libs/connectors/starter/static/schemas",
            "type": "FOLDER",
            "extensionFilter": "schema.xml"
        },
        {
            "id": "i2.chartingschemes",
            "value": "./libs/connectors/starter/static/chartingschemes",
            "type": "FOLDER",
            "extensionFilter": "cs.xml"
        },
        {
            "id": "i2.typemaps",
            "value": "./libs/connectors/starter/static/typemaps",
            "type": "FOLDER",
            "extensionFilter": "typemap.json"
        },
        {
            "id": "i2.transforms",
            "value": "./libs/connectors/starter/static/transforms",
            "type": "FOLDER",
            "extensionFilter": "transform.json"
        }
    ]
}